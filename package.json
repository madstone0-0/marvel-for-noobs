        "mini-css-extract-plugin": "^2.6.1",
        "prettier": "^2.7.1",
        "resolve-url-loader": "^5.0.0",
        "sass": "^1.56.0",
        "source-map": "^0.7.4",
        "style-loader": "^3.3.1",
