{
    "name": "marvel-for-noobs",
    "version": "0.4.1",
    "private": true,
    "dependencies": {
        "@artsy/fresnel": "6.1.0",
        "axios": "1.4.0",
        "classnames": "2.3.2",
        "express": "4.18.2",
        "prop-types": "^15.8.1",
        "react": "18.2.0",
        "react-countdown": "2.3.5",
        "react-dom": "18.2.0",
        "react-ga": "3.3.1",
        "react-router": "6.12.1",
        "react-router-dom": "6.12.0",
        "react-window": "^1.8.9",
        "semantic-ui-css": "2.5.0",
        "semantic-ui-react": "2.1.4",
        "universal-cookie": "4.0.4"
    },
    "scripts": {
        "build-css": "sass ./src/styles/style.scss ./src/styles/style.css --no-source-map --update --load-path='./src' --style=expanded ",
        "watch-css": "npm run build-css && sass ./src/styles/style.scss ./src/styles/style.css --no-source-map --load-path='./src' --style=expanded --watch",
        "dev": "vite",
        "build": "vite build",
        "surge-deploy": "npm run build && cd dist && surge"
    },
    "eslintConfig": {
        "extends": "react-app"
    },
    "browserslist": [
        ">0.2%",
        "not dead",
        "not ie <= 11",
        "not op_mini all"
    ],
    "devDependencies": {
        "@vitejs/plugin-react": "^4.0.0",
        "eslint": "^8.39.0",
        "eslint-config-prettier": "^8.6.0",
        "eslint-plugin-import": "^2.27.5",
        "eslint-plugin-jsx-a11y": "^6.7.1",
        "eslint-plugin-prettier": "^4.2.1",
        "eslint-plugin-react": "^7.32.2",
        "eslint-plugin-react-hooks": "^4.6.0",
        "prettier": "^2.8.8",
        "sass": "^1.62.1",
        "surge": "^0.23.1",
        "vite": "^4.3.9"
    }
}
