{
    "name": "marvel-for-noobs",
    "version": "0.4.0",
    "private": true,
    "dependencies": {
        "@artsy/fresnel": "6.1.0",
        "axios": "1.3.2",
        "classnames": "2.3.2",
        "express": "4.18.2",
        "prop-types": "^15.8.1",
        "react": "18.2.0",
        "react-countdown": "2.3.5",
        "react-dom": "18.2.0",
        "react-ga": "3.3.1",
        "react-router": "6.8.0",
        "react-router-dom": "6.8.2",
        "react-window": "^1.8.8",
        "semantic-ui-css": "2.5.0",
        "semantic-ui-react": "2.1.4",
        "universal-cookie": "4.0.4"
    },
    "scripts": {
        "build-css": "sass ./src/styles/style.scss ./src/styles/style.css --no-source-map --update --load-path='./src' --style=expanded ",
        "watch-css": "npm run build-css && sass ./src/styles/style.scss ./src/styles/style.css --no-source-map --load-path='./src' --style=expanded --watch",
        "start-dev": "webpack serve --config webpack.config.dev.js --hot",
        "build": "webpack --config webpack.config.prod.js",
        "start": "node server.js",
        "surge-deploy": "npm run build && cd dist && surge"
    },
    "eslintConfig": {
        "extends": "react-app"
    },
    "browserslist": [
        ">0.2%",
        "not dead",
        "not ie <= 11",
        "not op_mini all"
    ],
    "devDependencies": {
        "@babel/core": "^7.20.12",
        "@babel/eslint-parser": "^7.19.1",
        "@babel/eslint-plugin": "^7.19.1",
        "@babel/preset-env": "^7.20.2",
        "@babel/preset-react": "^7.18.6",
        "@squoosh/lib": "^0.5.3",
        "babel-loader": "^9.1.2",
        "clean-webpack-plugin": "^4.0.0",
        "copy-webpack-plugin": "^11.0.0",
        "css-loader": "^6.7.3",
        "css-minimizer-webpack-plugin": "^4.2.2",
        "eslint": "^8.33.0",
        "eslint-config-prettier": "^8.6.0",
        "eslint-plugin-import": "^2.27.5",
        "eslint-plugin-jsx-a11y": "^6.7.1",
        "eslint-plugin-prettier": "^4.2.1",
        "eslint-plugin-react": "^7.32.2",
        "eslint-plugin-react-hooks": "^4.6.0",
        "html-webpack-plugin": "^5.5.0",
        "image-minimizer-webpack-plugin": "^3.8.1",
        "less": "^4.1.3",
        "less-loader": "^11.1.0",
        "mini-css-extract-plugin": "^2.7.2",
        "prettier": "^2.8.3",
        "resolve-url-loader": "^5.0.0",
        "sass": "^1.58.0",
        "source-map": "^0.7.4",
        "style-loader": "^3.3.1",
        "surge": "^0.23.1",
        "terser-webpack-plugin": "^5.3.6",
        "webpack": "^5.75.0",
        "webpack-cli": "^5.0.1",
        "webpack-dev-server": "^4.11.1",
        "webpack-merge": "^5.8.0"
    }
