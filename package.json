{
    "name": "marvel-for-noobs",
    "version": "0.3.0",
    "private": true,
    "dependencies": {
        "@artsy/fresnel": "6.1.0",
        "axios": "1.1.3",
        "classnames": "2.3.2",
        "express": "4.18.2",
        "jquery": "3.6.1",
        "react": "18.2.0",
        "react-countdown": "2.3.3",
        "react-dom": "18.2.0",
        "react-ga": "3.3.1",
        "react-router": "6.4.3",
        "react-router-dom": "6.4.3",
        "semantic-ui-css": "2.5.0",
        "semantic-ui-react": "2.1.3",
        "universal-cookie": "4.0.4"
    },
    "scripts": {
        "build-css": "sass ./src/styles/style.scss ./src/styles/style.css --no-source-map --update --load-path='./src' --style=expanded ",
        "watch-css": "npm run build-css && sass ./src/styles/style.scss ./src/styles/style.css --no-source-map --load-path='./src' --style=expanded --watch",
        "start-dev": "webpack serve --config webpack.config.dev.js --hot",
        "build": "webpack --config webpack.config.prod.js",
        "start": "node server.js"
    },
    "eslintConfig": {
        "extends": "react-app"
    },
    "browserslist": [
        ">0.2%",
        "not dead",
        "not ie <= 11",
        "not op_mini all"
    ],
    "devDependencies": {
        "@babel/core": "^7.19.6",
        "@babel/eslint-parser": "^7.19.1",
        "@babel/eslint-plugin": "^7.19.1",
        "@babel/preset-env": "^7.19.4",
        "@babel/preset-react": "^7.18.6",
        "@squoosh/lib": "^0.4.0",
