{
    "name": "marvel-for-noobs",
    "version": "0.2.0",
    "private": true,
    "dependencies": {
        "antd": "3.26.0",
        "axios": "0.18.1",
        "logrocket": "1.0.3",
        "moment": "2.24.0",
        "moment-countdown": "0.0.3",
        "react": "16.12.0",
        "react-dom": "16.12.0",
        "react-ga": "2.7.0",
        "react-router": "5.1.2",
        "react-router-dom": "5.1.2",
        "react-scripts": "3.2.0",
        "semantic-ui-css": "2.4.1",
        "semantic-ui-react": "0.88.1"
    },
    "scripts": {
        "build-css": "node-sass --output-style expanded --include-path ./src --include-path ./node_modlues src/ -o src/",
        "watch-css": "npm run build-css && node-sass --output-style expanded --include ./src --include-path: ./node_modules src/ -o src/ --watch --recursive",
        "start": "react-scripts start",
        "build": "react-scripts build",
        "test": "react-scripts test",
        "eject": "react-scripts eject"
    },
    "eslintConfig": {
        "extends": "react-app"
    },
    "browserslist": [
        ">0.2%",
        "not dead",
        "not ie <= 11",
        "not op_mini all"
    ],
    "devDependencies": {
        "enzyme": "3.10.0",
        "enzyme-adapter-react-16": "1.15.1",
        "npm-run-all": "4.1.5",
        "react-test-renderer": "16.12.0"
    }
}
